<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:id="@+id/list_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/favicon"
        android:layout_width="@dimen/0_resource_name_obfuscated_res_0x7f080095"
        android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f080094"
        android:scaleType="center"/>
    <TextView
        android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150440"
        android:gravity="center_vertical"
        android:id="@+id/title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:layout_weight="1"
        android:textAlignment="viewStart"
        android:paddingEnd="16dp"/>
</LinearLayout>
