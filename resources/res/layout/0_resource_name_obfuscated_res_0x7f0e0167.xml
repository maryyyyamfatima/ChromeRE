<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_gravity="start|top"
        android:id="@+id/close_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/0_resource_name_obfuscated_res_0x7f090428"
        android:contentDescription="@string/0_resource_name_obfuscated_res_0x7f1405b4"/>
    <LinearLayout
        android:layout_gravity="center"
        android:orientation="vertical"
        android:id="0x7f010152"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:id="@+id/video_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <FrameLayout
                android:id="@+id/video_player"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <ProgressBar
                android:layout_gravity="center"
                android:id="@+id/loading_indicator"
                android:background="@android:color/transparent"
                android:visibility="gone"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:indeterminateDuration="1000"
                android:indeterminateTint="@android:color/white"/>
        </FrameLayout>
        <TextView
            android:textSize="@dimen/0_resource_name_obfuscated_res_0x7f0803ae"
            android:textColor="@color/0_resource_name_obfuscated_res_0x7f0705c4"
            android:id="@+id/video_title"
            android:paddingLeft="@dimen/0_resource_name_obfuscated_res_0x7f0803ac"
            android:paddingRight="@dimen/0_resource_name_obfuscated_res_0x7f0803ac"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/0_resource_name_obfuscated_res_0x7f0803ad"
            android:lines="2"
            android:fontFamily="sans-serif"
            android:paddingHorizontal="@dimen/0_resource_name_obfuscated_res_0x7f0803ac"/>
        <TextView
            android:textSize="@dimen/0_resource_name_obfuscated_res_0x7f0803a9"
            android:textColor="@color/0_resource_name_obfuscated_res_0x7f0705c4"
            android:id="@+id/video_attribution"
            android:paddingLeft="@dimen/0_resource_name_obfuscated_res_0x7f0803ac"
            android:paddingRight="@dimen/0_resource_name_obfuscated_res_0x7f0803ac"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/0_resource_name_obfuscated_res_0x7f0803ad"
            android:drawablePadding="@dimen/0_resource_name_obfuscated_res_0x7f0803aa"
            android:paddingHorizontal="@dimen/0_resource_name_obfuscated_res_0x7f0803ac"/>
    </LinearLayout>
</FrameLayout>
