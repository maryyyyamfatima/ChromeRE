<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="center_horizontal"
    android:orientation="vertical"
    android:id="@+id/item"
    android:background="@drawable/0_resource_name_obfuscated_res_0x7f0903df"
    android:layout_width="@dimen/0_resource_name_obfuscated_res_0x7f080602"
    android:layout_height="280dp"
    android:layout_marginBottom="24dp"
    android:importantForAccessibility="yes"
    android:layout_marginStart="@dimen/0_resource_name_obfuscated_res_0x7f0805ff"
    android:layout_marginEnd="@dimen/0_resource_name_obfuscated_res_0x7f0805ff">
    <org.chromium.chrome.browser.content_creation.notes.LineLimitedTextView
        android:ellipsize="end"
        android:id="@+id/text"
        android:background="@drawable/0_resource_name_obfuscated_res_0x7f0903df"
        android:layout_width="240dp"
        android:layout_height="200dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:importantForAccessibility="no"
        android:paddingStart="5dp"
        android:paddingEnd="5dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"/>
    <RelativeLayout
        android:id="@+id/footer"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingStart="20dp"
        android:paddingEnd="8dp">
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150473"
            android:ellipsize="end"
            android:id="@+id/footer_link"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:importantForAccessibility="no"
            android:layout_marginEnd="20dp"
            android:layout_alignParentStart="true"/>
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150472"
            android:ellipsize="end"
            android:id="@+id/footer_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:layout_alignTop="@+id/footer_icon"
            android:importantForAccessibility="no"
            android:layout_marginEnd="20dp"
            android:layout_alignParentStart="false"/>
        <ImageView
            android:id="@+id/footer_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/0_resource_name_obfuscated_res_0x7f0901d9"
            android:layout_alignParentBottom="true"
            android:importantForAccessibility="no"
            android:layout_alignParentEnd="true"/>
    </RelativeLayout>
</LinearLayout>
