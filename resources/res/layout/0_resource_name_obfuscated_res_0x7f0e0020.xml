        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150452"
            android:ellipsize="end"
            android:id="@+id/secondary_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"/>
    </LinearLayout>
    <ImageButton
        android:id="@+id/psl_info_btn"
        android:background="?attr/0_resource_name_obfuscated_res_0x7f0503f1"
        android:padding="4dp"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/0_resource_name_obfuscated_res_0x7f0900c9"
        android:contentDescription="@string/0_resource_name_obfuscated_res_0x7f1405af"/>
</LinearLayout>
