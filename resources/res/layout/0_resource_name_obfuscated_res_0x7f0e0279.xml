<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f08072d">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f08072d">
        <View
            android:layout_width="match_parent"
            android:layout_height="4dp"/>
        <RelativeLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:paddingStart="16dp"
            android:paddingEnd="16dp">
            <org.chromium.ui.widget.ChromeImageView
                android:id="@+id/favicon"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginBottom="14dp"
                android:scaleType="fitCenter"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="16dp"/>
            <ImageView
                android:layout_gravity="top|center_horizontal"
                android:id="@+id/drag_handlebar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:src="@drawable/0_resource_name_obfuscated_res_0x7f090168"
                android:layout_centerHorizontal="true"
                android:importantForAccessibility="no"/>
            <org.chromium.ui.widget.ChromeImageView
                android:id="@+id/close"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginBottom="6dp"
                android:src="@drawable/0_resource_name_obfuscated_res_0x7f0900c5"
                android:layout_alignParentBottom="true"
                android:contentDescription="@string/0_resource_name_obfuscated_res_0x7f140362"
                android:paddingStart="8dp"
                android:paddingEnd="8dp"
                android:layout_alignParentEnd="true"
                app:tint="?attr/0_resource_name_obfuscated_res_0x7f05012e"/>
            <org.chromium.ui.widget.ChromeImageView
                android:id="@+id/open_in_new_tab"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:visibility="gone"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginBottom="6dp"
                android:src="@drawable/0_resource_name_obfuscated_res_0x7f0903f4"
                android:layout_alignParentBottom="true"
                android:contentDescription="@string/0_resource_name_obfuscated_res_0x7f1403ac"
                android:paddingStart="8dp"
                android:paddingEnd="8dp"
                android:layout_marginEnd="8dp"
                android:layout_toStartOf="@+id/close"
                app:tint="?attr/0_resource_name_obfuscated_res_0x7f05012e"/>
            <TextView
                android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f15044d"
                android:ellipsize="end"
                android:id="@+id/title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:singleLine="true"
                android:textAlignment="viewStart"
                android:layout_marginEnd="16dp"
                android:layout_toStartOf="@+id/open_in_new_tab"
                android:layout_toEndOf="@+id/favicon"/>
            <org.chromium.ui.widget.ChromeImageView
                android:id="@+id/security_icon"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_marginTop="3dp"
                android:layout_below="@+id/title"
                android:layout_alignStart="@+id/title"
                app:tint="?attr/0_resource_name_obfuscated_res_0x7f05012e"/>
            <TextView
                android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150452"
                android:ellipsize="start"
                android:id="@+id/origin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:layout_below="@+id/title"
                android:textAlignment="viewStart"
                android:layout_marginStart="4dp"
                android:layout_toStartOf="@+id/open_in_new_tab"
                android:layout_toEndOf="@+id/security_icon"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"/>
        <ProgressBar
            android:id="@+id/progress_bar"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:max="100"
            style="?android:attr/progressBarStyleHorizontal"/>
    </LinearLayout>
    <org.chromium.components.browser_ui.widget.FadingShadowView
        android:id="@+id/shadow"
        android:layout_width="match_parent"
        android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f08005f"
        android:layout_marginTop="@dimen/0_resource_name_obfuscated_res_0x7f08072d"/>
</FrameLayout>
