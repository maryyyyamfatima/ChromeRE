<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/list_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/subtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="@string/0_resource_name_obfuscated_res_0x7f1405a9"
        android:textDirection="locale"
        android:layout_marginStart="24dp"
        app:leading="@dimen/0_resource_name_obfuscated_res_0x7f0807cc"
        style="@style/0_resource_name_obfuscated_res_0x7f150452"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/app_language_prompt_content_recycler_view"
            android:scrollbars="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:layout_gravity="top"
            android:id="@+id/top_shadow"
            android:layout_width="match_parent"
            android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f08080c"
            android:src="@drawable/0_resource_name_obfuscated_res_0x7f090397"
            android:scaleType="fitXY"
            android:importantForAccessibility="no"/>
        <ImageView
            android:layout_gravity="bottom"
            android:id="@+id/bottom_shadow"
            android:layout_width="match_parent"
            android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f08080c"
            android:src="@drawable/0_resource_name_obfuscated_res_0x7f090397"
            android:scaleType="fitXY"
            android:scaleY="-1"
            android:importantForAccessibility="no"/>
    </FrameLayout>
</LinearLayout>
