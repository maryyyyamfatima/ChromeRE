<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/dino_quick_action_area"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:gravity="fill"
        android:orientation="vertical"
        android:id="@+id/dino_quick_action_button"
        android:background="@drawable/0_resource_name_obfuscated_res_0x7f09045f"
        android:paddingTop="@dimen/0_resource_name_obfuscated_res_0x7f0806b6"
        android:paddingBottom="@dimen/0_resource_name_obfuscated_res_0x7f0806b6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingStart="@dimen/0_resource_name_obfuscated_res_0x7f0806b5">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/0_resource_name_obfuscated_res_0x7f090460"
            android:scaleType="fitCenter"
            android:adjustViewBounds="true"
            android:importantForAccessibility="no"/>
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150440"
            android:gravity="bottom"
            android:id="@+id/dino_quick_action_text"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:text="@string/0_resource_name_obfuscated_res_0x7f140400"
            android:maxLines="1"
            android:layout_weight="1"
            android:textAlignment="viewStart"/>
    </LinearLayout>
</FrameLayout>
