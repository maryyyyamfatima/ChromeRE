<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/start_dropdown_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/0_resource_name_obfuscated_res_0x7f0801de"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:id="@+id/dropdown_label_wrapper"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1">
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150440"
            android:ellipsize="end"
            android:id="@+id/dropdown_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:includeFontPadding="false"
            android:textAlignment="viewStart"
            android:layout_marginStart="@dimen/0_resource_name_obfuscated_res_0x7f0801e1"
            android:layout_marginEnd="@dimen/0_resource_name_obfuscated_res_0x7f0801e1"/>
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150470"
            android:ellipsize="end"
            android:id="@+id/dropdown_sublabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:includeFontPadding="false"
            android:textAlignment="viewStart"
            android:layout_marginStart="@dimen/0_resource_name_obfuscated_res_0x7f0801e1"
            android:layout_marginEnd="@dimen/0_resource_name_obfuscated_res_0x7f0801e1"/>
    </LinearLayout>
    <ImageView
        android:id="@+id/end_dropdown_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"/>
</LinearLayout>
