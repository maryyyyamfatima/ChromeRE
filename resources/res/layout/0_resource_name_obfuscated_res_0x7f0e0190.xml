<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150421"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:id="@+id/mr_chooser_title"
        android:paddingLeft="24dp"
        android:paddingTop="12dp"
        android:paddingRight="24dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="52dp"
        android:text="@string/0_resource_name_obfuscated_res_0x7f140676"
        android:singleLine="true"/>
    <ListView
        android:id="@+id/mr_chooser_list"
        android:paddingBottom="24dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@android:color/transparent"
        android:dividerHeight="0dp"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@android:id/empty"
        android:paddingLeft="16dp"
        android:paddingTop="90dp"
        android:paddingRight="16dp"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="240dp">
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150420"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/0_resource_name_obfuscated_res_0x7f140675"/>
        <ProgressBar
            android:layout_gravity="center"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:indeterminate="true"
            style="?android:attr/progressBarStyleHorizontal"/>
    </LinearLayout>
</LinearLayout>
