<?xml version="1.0" encoding="utf-8"?>
<org.chromium.chrome.browser.feed.followmanagement.FollowManagementItemView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:layout_gravity="center_vertical"
        android:id="@+id/follow_management_favicon"
        android:layout_width="@dimen/0_resource_name_obfuscated_res_0x7f080843"
        android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f080843"
        android:layout_margin="@dimen/0_resource_name_obfuscated_res_0x7f080243"
        android:src="@drawable/0_resource_name_obfuscated_res_0x7f09049f"
        android:scaleType="center"/>
    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/0_resource_name_obfuscated_res_0x7f080242"
        android:layout_marginBottom="@dimen/0_resource_name_obfuscated_res_0x7f080242"
        android:layout_weight="1"
        android:layout_marginVertical="@dimen/0_resource_name_obfuscated_res_0x7f080242">
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f15043f"
            android:id="@+id/follow_management_item_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150452"
            android:ellipsize="end"
            android:id="@+id/follow_management_item_url"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxLines="2"
            android:layout_below="@+id/follow_management_item_text"/>
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150466"
            android:id="@+id/follow_management_item_status"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:layout_below="@+id/follow_management_item_url"/>
    </RelativeLayout>
    <CheckBox
        android:layout_gravity="center_vertical"
        android:id="@+id/follow_management_subscribed_checkbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"/>
</org.chromium.chrome.browser.feed.followmanagement.FollowManagementItemView>
