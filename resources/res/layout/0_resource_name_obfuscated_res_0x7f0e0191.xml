<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:paddingLeft="@dimen/0_resource_name_obfuscated_res_0x7f0804c6"
    android:paddingTop="@dimen/0_resource_name_obfuscated_res_0x7f0804c7"
    android:paddingRight="@dimen/0_resource_name_obfuscated_res_0x7f0804c5"
    android:paddingBottom="@dimen/0_resource_name_obfuscated_res_0x7f0804c4"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="32dp"
    android:paddingStart="@dimen/0_resource_name_obfuscated_res_0x7f0804c6"
    android:paddingEnd="@dimen/0_resource_name_obfuscated_res_0x7f0804c5">
    <ImageView
        android:id="@+id/mr_chooser_route_icon"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginEnd="24dp"/>
    <ProgressBar
        android:id="@+id/mr_chooser_route_progress_bar"
        android:visibility="gone"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginRight="24dp"
        android:indeterminate="true"
        android:layout_marginEnd="24dp"
        style="?android:attr/progressBarStyle"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp">
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f15041f"
            android:ellipsize="marquee"
            android:gravity="start"
            android:id="@+id/mr_chooser_route_name"
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:singleLine="true"
            android:textAlignment="viewStart"/>
        <TextView
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150420"
            android:ellipsize="marquee"
            android:gravity="start"
            android:id="@+id/mr_chooser_route_desc"
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:singleLine="true"
            android:textAlignment="viewStart"/>
    </LinearLayout>
</LinearLayout>
