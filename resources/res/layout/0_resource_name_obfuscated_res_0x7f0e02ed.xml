<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/icon_frame"
        layout="@layout/0_resource_name_obfuscated_res_0x7f0e02f0"/>
    <TextView
        android:ellipsize="end"
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/time"
        android:layout_toEndOf="@+id/icon_frame"
        style="@style/0_resource_name_obfuscated_res_0x7f150613"/>
    <TextView
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:layout_alignBaseline="@+id/title"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_alignParentEnd="true"
        style="@style/0_resource_name_obfuscated_res_0x7f150612"/>
    <RelativeLayout
        android:id="@+id/body_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-4dp"
        android:layout_marginBottom="-4dp"
        android:layout_below="@+id/title"
        android:layout_toEndOf="@+id/icon_frame">
        <TextView
            android:ellipsize="end"
            android:id="@+id/body"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:layout_alignParentTop="true"
            android:layout_alignWithParentIfMissing="true"
            android:layout_marginEnd="8dp"
            android:layout_toStartOf="@+id/work_profile_badge"
            android:layout_alignParentStart="true"
            style="@style/0_resource_name_obfuscated_res_0x7f150608"/>
        <ImageView
            android:id="@+id/work_profile_badge"
            android:visibility="gone"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginTop="1dp"
            android:scaleType="centerInside"
            android:layout_alignParentTop="true"
            android:contentDescription="@string/0_resource_name_obfuscated_res_0x7f1406fb"
            android:layout_marginEnd="8dp"
            android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <TextView
        android:ellipsize="start"
        android:id="@+id/origin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:layout_below="@+id/body_container"
        android:layout_marginEnd="8dp"
        android:layout_toStartOf="@+id/small_icon_footer"
        android:layout_toEndOf="@+id/icon_frame"
        style="@style/0_resource_name_obfuscated_res_0x7f15060e"/>
    <ViewStub
        android:id="@+id/small_icon_footer"
        android:layout="@layout/0_resource_name_obfuscated_res_0x7f0e02f1"
        android:inflatedId="@+id/small_icon_footer"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_below="@+id/body_container"
        android:layout_marginEnd="8dp"
        android:layout_alignParentEnd="true"/>
</RelativeLayout>
