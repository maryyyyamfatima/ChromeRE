<?xml version="1.0" encoding="utf-8"?>
<org.chromium.chrome.browser.share.screenshot.ScreenshotShareSheetView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
                <org.chromium.ui.widget.ChromeImageView
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/screenshot"
                    android:padding="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </FrameLayout>
            <LinearLayout
                android:gravity="bottom"
                android:layout_gravity="start|bottom"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:weightSum="4"
                android:layout_marginStart="36dp"
                android:layout_marginEnd="36dp">
                <TextView
                    android:gravity="center"
                    android:layout_gravity="end|bottom"
                    android:id="@+id/edit"
                    android:background="?attr/0_resource_name_obfuscated_res_0x7f0503f2"
                    android:text="@string/0_resource_name_obfuscated_res_0x7f1409b0"
                    android:drawableTop="@drawable/0_resource_name_obfuscated_res_0x7f09016e"
                    android:layout_weight="1"
                    android:drawableTint="@color/0_resource_name_obfuscated_res_0x7f07012b"
                    style="@style/0_resource_name_obfuscated_res_0x7f1502d4"/>
                <TextView
                    android:gravity="center"
                    android:layout_gravity="bottom|center_horizontal"
                    android:id="@+id/delete"
                    android:background="?attr/0_resource_name_obfuscated_res_0x7f0503f2"
                    android:text="@string/0_resource_name_obfuscated_res_0x7f1409ae"
                    android:drawableTop="@drawable/0_resource_name_obfuscated_res_0x7f090156"
                    android:layout_weight="1"
                    android:drawableTint="@color/0_resource_name_obfuscated_res_0x7f07012b"
                    style="@style/0_resource_name_obfuscated_res_0x7f1502d4"/>
                <TextView
                    android:gravity="center"
                    android:layout_gravity="bottom|center_horizontal"
                    android:id="@+id/save"
                    android:background="?attr/0_resource_name_obfuscated_res_0x7f0503f2"
                    android:text="@string/0_resource_name_obfuscated_res_0x7f1409c1"
                    android:drawableTop="@drawable/0_resource_name_obfuscated_res_0x7f090479"
                    android:layout_weight="1"
                    android:drawableTint="@color/0_resource_name_obfuscated_res_0x7f07012b"
                    style="@style/0_resource_name_obfuscated_res_0x7f1502d4"/>
                <TextView
                    android:gravity="center"
                    android:layout_gravity="start|bottom"
                    android:id="@+id/share"
                    android:background="?attr/0_resource_name_obfuscated_res_0x7f0503f2"
                    android:text="@string/0_resource_name_obfuscated_res_0x7f1409c2"
                    android:drawableTop="@drawable/0_resource_name_obfuscated_res_0x7f09048e"
                    android:layout_weight="1"
                    android:drawableTint="@color/0_resource_name_obfuscated_res_0x7f07012b"
                    style="@style/0_resource_name_obfuscated_res_0x7f1502d4"/>
            </LinearLayout>
        </LinearLayout>
        <org.chromium.ui.widget.ChromeImageButton
            android:id="@+id/close_button"
            android:src="@drawable/0_resource_name_obfuscated_res_0x7f0900c5"
            android:contentDescription="@string/0_resource_name_obfuscated_res_0x7f140362"
            app:tint="@color/0_resource_name_obfuscated_res_0x7f07012b"
            style="@style/0_resource_name_obfuscated_res_0x7f1505fe"/>
    </RelativeLayout>
</org.chromium.chrome.browser.share.screenshot.ScreenshotShareSheetView>
