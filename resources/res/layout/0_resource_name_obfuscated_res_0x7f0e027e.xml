<?xml version="1.0" encoding="utf-8"?>
<org.chromium.chrome.browser.ui.signin.fre.SigninFirstRunView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:fillViewport="true"
        android:layout_above="@+id/fre_footer_viewgroup"
        android:layout_alignParentTop="true"
        android:requiresFadingEdge="vertical">
        <LinearLayout
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:id="0x7f010343"
                android:visibility="invisible"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="3"/>
            <ImageView
                android:id="@+id/fre_logo"
                android:layout_width="match_parent"
                android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f080250"
                android:layout_marginBottom="24dp"
                android:src="@drawable/0_resource_name_obfuscated_res_0x7f090181"
                android:importantForAccessibility="no"/>
            <TextView
                android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f1503dd"
                android:gravity="center"
                android:id="@+id/title"
                android:paddingLeft="24dp"
                android:paddingRight="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/0_resource_name_obfuscated_res_0x7f1404c6"
                android:lineSpacingMultiplier="1.2"
                android:paddingHorizontal="24dp"/>
            <org.chromium.ui.widget.TextViewWithLeading
                android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f150452"
                android:gravity="center"
                android:id="@+id/subtitle"
                android:paddingLeft="60dp"
                android:paddingTop="8dp"
                android:paddingRight="60dp"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingHorizontal="60dp"
                app:leading="@dimen/0_resource_name_obfuscated_res_0x7f0807cc"/>
            <FrameLayout
                android:id="@+id/fre_browser_managed_by_organization"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp">
                <include
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    layout="@layout/0_resource_name_obfuscated_res_0x7f0e0116"/>
            </FrameLayout>
            <View
                android:id="0x7f010344"
                android:visibility="invisible"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="5"/>
            <ProgressBar
                android:id="@+id/fre_native_and_policy_load_progress_spinner"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginBottom="128dp"
                android:alpha="0"
                style="@style/0_resource_name_obfuscated_res_0x7f15064e"/>
            <include layout="@layout/0_resource_name_obfuscated_res_0x7f0e027c"/>
        </LinearLayout>
    </ScrollView>
    <FrameLayout
        android:id="@+id/fre_footer_viewgroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true">
        <org.chromium.ui.widget.TextViewWithClickableSpans
            android:textAppearance="@style/0_resource_name_obfuscated_res_0x7f15046f"
            android:gravity="center_horizontal"
            android:id="@+id/signin_fre_footer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="24dp"
            android:text="@string/0_resource_name_obfuscated_res_0x7f140a5e"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:leading="@dimen/0_resource_name_obfuscated_res_0x7f0807ce"/>
    </FrameLayout>
    <ProgressBar
        android:id="@+id/fre_signin_progress_spinner"
        android:visibility="gone"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginBottom="128dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        style="@style/0_resource_name_obfuscated_res_0x7f15064e"/>
    <TextView
        android:gravity="center"
        android:id="@+id/fre_signin_progress_text"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:text="@string/0_resource_name_obfuscated_res_0x7f1404c3"
        android:layout_alignParentBottom="true"
        style="@style/0_resource_name_obfuscated_res_0x7f150452"/>
</org.chromium.chrome.browser.ui.signin.fre.SigninFirstRunView>
