            android:layout_gravity="center_vertical"
            android:id="@+id/desc"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_marginBottom="10dp"
            android:maxLines="1"
            app:leading="@dimen/0_resource_name_obfuscated_res_0x7f0807cc"/>
