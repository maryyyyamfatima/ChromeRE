<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/transition_frame"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageButton
        android:id="@+id/back_button"
        android:src="@drawable/0_resource_name_obfuscated_res_0x7f09042a"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        style="@style/0_resource_name_obfuscated_res_0x7f150600"/>
    <RelativeLayout
        android:id="0x7f010838"
        android:paddingBottom="48dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">
        <ImageView
            android:id="@+id/transition_icon"
            android:padding="4dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/0_resource_name_obfuscated_res_0x7f0904d7"
            android:scaleType="center"
            android:layout_centerHorizontal="true"/>
        <TextView
            android:textSize="16sp"
            android:textColor="#ffffff"
            android:gravity="center"
            android:id="@+id/transition_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:text="@string/0_resource_name_obfuscated_res_0x7f14083c"
            android:layout_below="@+id/transition_icon"/>
    </RelativeLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/transition_bottom_frame"
        android:background="#ff546e7a"
        android:layout_width="match_parent"
        android:layout_height="@dimen/0_resource_name_obfuscated_res_0x7f080826"
        android:layout_alignParentBottom="true">
        <TextView
            android:textSize="16sp"
            android:textColor="#ffffff"
            android:gravity="start|center_vertical"
            android:id="0x7f010834"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/0_resource_name_obfuscated_res_0x7f140ace"
            android:layout_below="@+id/transition_icon"
            android:paddingEnd="20dp"
            android:layout_marginStart="24dp"/>
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"/>
        <View
            android:id="@+id/divider"
            android:background="#1f000000"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"/>
        <TextView
            android:textSize="16sp"
            android:textColor="#ffffff"
            android:gravity="end|center_vertical"
            android:id="@+id/transition_switch_action"
            android:background="?android:attr/selectableItemBackground"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/0_resource_name_obfuscated_res_0x7f140acd"
            android:textAllCaps="true"
            android:paddingStart="20dp"
            android:paddingEnd="24dp"/>
    </LinearLayout>
</RelativeLayout>
